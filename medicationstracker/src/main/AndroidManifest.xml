<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpeechRecognitionApp">
        <activity
            android:name=".AdminActivity"
            android:exported="true"
            android:label="@string/title_activity_admin"
            android:theme="@style/Theme.SpeechRecognitionApp" />
        <activity
            android:name=".DoctorActivity"
            android:exported="true"
            android:label="@string/title_activity_doctor"
            android:theme="@style/Theme.SpeechRecognitionApp" />
        <activity
            android:name=".ApplicationActivity"
            android:exported="true"
            android:label="@string/title_activity_application"
            android:theme="@style/Theme.SpeechRecognitionApp" />
        <activity
            android:name=".HeadActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SpeechRecognitionApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>